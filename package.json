    "typescript",
    "prediction"
  ],
  "author": "Rafael Franco",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.9",
    "crypto-js": "^4.2.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^22.13.1",
    "prettier": "^3.4.2",
    "semantic-release": "^24.2.1",
    "typescript": "^5.7.3"
  }
}
